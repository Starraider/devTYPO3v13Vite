page {
    theme {
        language.enable = {$site.settings.page.theme.language.enable}
        googleFont.enable = {$site.settings.page.theme.googleFont.enable}
        cookieconsent.enable = {$site.settings.page.theme.cookieconsent.enable}
        breadcrumb.enableLevel = {$site.settings.page.theme.breadcrumb.enableLevel}
        copyright {
            enable = {$site.settings.page.theme.copyright.enable}
            text = {$site.settings.page.theme.copyright.text}
        }
    }
    logo {
        file = {$site.settings.page.logo.file}
        fileInverted = {$site.settings.page.logo.fileInverted}
        height = {$site.settings.page.logo.height}
        width = {$site.settings.page.logo.width}
        alt = {$site.settings.page.logo.alt}
        linktitle = {$site.settings.page.logo.linktitle}
    }
}
