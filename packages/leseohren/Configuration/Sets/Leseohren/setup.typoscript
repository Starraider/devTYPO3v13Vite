plugin.tx_leseohren_personen {
    settings {
      pageIDs {
        personShowPid = {$site.settings.pageIDs.personShowPid}
        organizationShowPid = {$site.settings.pageIDs.organizationShowPid}
        eventShowPid = {$site.settings.pageIDs.eventShowPid}
        blackboardShowPid = {$site.settings.pageIDs.blackboardShowPid}
        presentShowPid = {$site.settings.pageIDs.presentShowPid}
        registrationShowPid = {$site.settings.pageIDs.registrationShowPid}
      }
    }
    view {
        templateRootPaths.0 = {$site.settings.plugin.tx_leseohren_personen.view.templateRootPath}
        partialRootPaths.0 = {$site.settings.plugin.tx_leseohren_personen.view.partialRootPath}
        layoutRootPaths.0 = {$site.settings.plugin.tx_leseohren_personen.view.layoutRootPath}
    }
    persistence {
        storagePid = {$site.settings.persistence.storagePids.persons}
        recursive = 1
        classes {
            SKom\Leseohren\Domain\Model\Blackboard {
                newRecordStoragePid = {$site.settings.persistence.storagePids.blackboards}
            }
            SKom\Leseohren\Domain\Model\Gift {
                newRecordStoragePid = {$site.settings.persistence.storagePids.gifts}
            }
            SKom\Leseohren\Domain\Model\Present {
                newRecordStoragePid = {$site.settings.persistence.storagePids.gifts}
            }
        }
    }
    features.ignoreAllEnableFieldsInBe = 0
}

